
public class Matrices {
	public static void main(String[] args){
		double[][] matriz={{1,2,3}, {4,5,6}, {7,8,9}};
		
		Cmatrices mat= new Cmatrices();
		
		mat.Determinante(0, matriz);
		
		try{
			System.out.println("La determinante de la matriz es:" + mat);
		}catch(Exception e){
			System.out.println("Todo esta mal");	
		}
	}
}
